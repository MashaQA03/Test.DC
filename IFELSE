public class IFELSE {
    public static void main(String [] args) {
        int age = 35;

        if (age<=3){
            System.out.println("Baby");
        } else if (age >= 4 && age <= 14){
            System.out.println("Kid");
        } else if (age >= 15 && age <=18) {
            System.out.println ("Teen");
        } else
            System.out.println("Adult");
    }
}
